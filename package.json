{
  "name": "electron",
  "version": "1.0.0",
  "description": "My-APP",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "start": "electron .",
    "dev": "electronmon .",
	"build": "electron-builder"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^24.1.3",
    "electron-builder": "^23.6.0",
    "electronmon": "^2.0.2"
  },
  "build": {
	"appId": "my-app",
	"win": {
		"target": "nsis",
		"icon": "./google_meet_icon.ico"
	},
	"directories": {
		"output": "./electron-build"
	},
	"nsis": {
		"oneClick": false,
		"installerIcon": "./google_meet_icon.ico",
		"uninstallerIcon": "./google_meet_icon.ico",
		"uninstallDisplayName": "uninstall electron",
		"license": "./license.md",
		"allowToChangeInstallationDirectory": true
	}
  }
}
