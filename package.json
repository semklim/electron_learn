{
  "name": "semklim-app",
  "version": "1.0.0",
  "description": "learn Electron",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "dev": "electronmon .",
    "build": "electron-builder"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^23.1.4",
    "electron-builder": "^23.6.0",
    "electronmon": "^2.0.2"
  },
  "dependencies": {
    "electron-updater": "^5.3.0"
  },
  "repository": "https://github.com/semklim/electron_learn",
  "publish": {
    "provider": "github",
    "releaseType": "release"
  },
  "build": {
    "appId": "my-app",
    "artifactName": "${name}-${version}.${ext}",
    "win": {
      "target": "nsis",
      "icon": "./google_meet_icon.ico"
    },
      "directories": {
        "output": "./electron-build"
      },
      "nsis": {
        "installerIcon": "./google_meet_icon.ico"
    }
  }
}
